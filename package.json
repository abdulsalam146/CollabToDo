{
  "name": "collabtodo",
  "version": "1.0.0",
  "description": "Real-time collaborative todo app with Supabase",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "python -m http.server 8000",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --ext .js,.ts --fix",
    "format": "prettier --write src/**/*.{js,css,html}",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link --project-ref your-project-id",
    "supabase:migrate": "supabase db push",
    "supabase:deploy": "supabase functions deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "todo",
    "collaborative",
    "real-time",
    "supabase",
    "javascript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "vite": "^4.4.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "@types/node": "^20.4.5",
    "tailwindcss": "^3.3.0",
    "postcss": "^8.4.24",
    "autoprefixer": "^10.4.14"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/collabtodo.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/collabtodo/issues"
  },
  "homepage": "https://github.com/yourusername/collabtodo#readme"
}